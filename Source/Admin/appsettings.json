{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimiting": {
    "Read": {
      "PermitLimit": 200,
      "WindowMinutes": 1,
      "SegmentsPerWindow": 6,
      "QueueLimit": 10
    },
    "Write": {
      "PermitLimit": 50,
      "WindowMinutes": 1,
      "SegmentsPerWindow": 6,
      "QueueLimit": 5
    },
    "Sensitive": {
      "PermitLimit": 5,
      "WindowMinutes": 1,
      "SegmentsPerWindow": 2,
      "QueueLimit": 0
    }
  },
  "InternalApi": {
    "ApiKey": "internal-service-key-dev-only",
    "ServiceName": "AdminService"
  },
  "Jwt": {
    "Issuer": "VttTools.Auth",
    "Audience": "VttTools.Services"
  },
  "AuditLogging": {
    "Enabled": true,
    "ExcludedPaths": [
      "/health",
      "/alive",
      "/ready",
      "/api/admin/audit"
    ]
  },
  "PublicLibrary": {
    "MasterUserId": "019639ea-c7de-7a01-8548-41edfccde206"
  },
  "AI": {
    "DefaultProviders": {
      "Image": "OpenAI",
      "Audio": "ElevenLabs",
      "Video": "RunwayML",
      "Prompt": "OpenAI",
      "Text": "OpenAI"
    },
    "Images": {
      "Token": {
        "ImageSize": 256,
        "Provider": "OpenAI",
        "Model": "gpt-image-1.5",
        "AspectRatio": "1:1",
        "MainPrompt": "a close-up of the main features of the subject, with a solid neutral background, to be used as a token on a virtual battlemap",
        "NegativePrompt": "partial head, cut off, background elements, scenery, landscape, environment details"
      },
      "Portrait": {
        "ImageSize": 1024,
        "Provider": "OpenAI",
        "Model": "gpt-image-1.5",
        "AspectRatio": "2:3",
        "MainPrompt": "a portrait of the subject, displaying the full view of the subject, with an interesting background, to be used as a display of the subject",
        "NegativePrompt": "cropped parts, distorted features, incorrect anatomy or perspective"
      }
    },
    "Providers": {
      "OpenAI": {
        "BaseUrl": "https://api.openai.com",
        "Health": "/v1/models",
        "gpt-5-mini": "/v1/responses",
        "gpt-5-nano": "/v1/responses",
        "gpt-image-1": "v1/images/generations",
        "gpt-image-1-mini": "v1/images/generations"
      },
      "Stability": {
        "BaseUrl": "https://api.stability.ai",
        "Health": "/v1/engines/list",
        "sd3": "/v2beta/stable-image/generate/sd3",
        "core": "/v2beta/stable-image/generate/core"
      },
      "Google": {
        "BaseUrl": "https://generativelanguage.googleapis.com/",
        "Health": "/v1/models",
        "gemini-2.5-flash-image": "/v1beta/models/gemini-2.5-flash-image:generateContent"
      },
      "ElevenLabs": {
        "BaseUrl": "https://api.elevenlabs.io",
        "Health": "/v1/models",
        "eleven_multilingual_v2": "/v1/text-to-speech"
      },
      "Suno": {
        "BaseUrl": "https://api.suno.ai",
        "Health": "/v1/models",
        "chirp-v3": "/v1/generate"
      },
      "RunwayML": {
        "BaseUrl": "https://api.runwayml.com",
        "Health": "/v1/models",
        "gen-3": "/v1/generate"
      }
    }
  }
}
