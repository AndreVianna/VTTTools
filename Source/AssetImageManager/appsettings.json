{
  "OutputFolder": "Output",
  "DelayBetweenRequestsInMs": 500,
  "MaxEntriesPerFile": 1024,
  "PromptEnhancer": {
    "Provider": "OpenAI",
    "Model": "gpt-5-nano"
  },
  "Images": {
    "TopDown": {
      "ImageSize": 256,
      "Provider": "Google",
      "Model": "gemini-2.5-flash-image",
      "AspectRatio": "1:1",
      "Background": "transparent"
    },
    "Photo": {
      "ImageSize": 256,
      "Provider": "Google",
      "Model": "gemini-2.5-flash-image",
      "AspectRatio": "1:1"
    },
    "Portrait": {
      "ImageSize": 1024,
      "Provider": "Google",
      "Model": "gemini-2.5-flash-image",
      "AspectRatio": "2:3"
    }
  },
  "Providers": {
    "Stability": {
      "BaseUrl": "https://api.stability.ai",
      "Health": "/v1/engines/list",
      "sd35": "/v2beta/stable-image/generate/sd3",
      "core": "/v2beta/stable-image/generate/core"
    },
    "OpenAI": {
      "BaseUrl": "https://api.openai.com",
      "Health": "/v1/models",
      "gpt-5-mini": "/v1/responses",
      "gpt-5-nano": "/v1/responses",
      "gpt-image-1": "v1/images/generations",
      "gpt-image-1-mini": "v1/images/generations"
    },
    "Google": {
      "BaseUrl": "https://generativelanguage.googleapis.com/",
      "Health": "/v1/models",
      "gemini-2.5-flash-image": "/v1beta/models/gemini-2.5-flash-image:generateContent"
    }
  }
}
