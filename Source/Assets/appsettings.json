{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "InternalApi": {
    "ApiKey": "internal-service-key-dev-only",
    "ServiceName": "AssetsService"
  },
  "Jwt": {
    "Issuer": "VttTools.Auth",
    "Audience": "VttTools.Services"
  },
  "AuditLogging": {
    "Enabled": true,
    "ExcludedPaths": [
      "/health",
      "/alive",
      "/ready"
    ]
  }
}
