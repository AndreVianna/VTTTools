{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "AppHost [https] (Development)",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}\\Source\\AppHost\\AppHost.csproj",
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+https?://\\S",
                "uriFormat": "http://localhost:17276"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "DOTNET_ENVIRONMENT": "Development",
                "DOTNET_DASHBOARD_OTLP_ENDPOINT_URL": "https://localhost:21172",
                "DOTNET_RESOURCE_SERVICE_ENDPOINT_URL": "https://localhost:22219"
            }
        }
    ]
}